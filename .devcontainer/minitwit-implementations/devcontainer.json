{
	"name": "minitwit-implementations",
	"dockerComposeFile": [
		"./compose.yaml"
	],
	"service": "minitwit-implementations",
	"workspaceFolder": "/workspaces/",
	"postCreateCommand": "bash .devcontainer/minitwit-implementations/post-create.sh",
	"remoteUser": "root",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.black-formatter",
				"ms-python.debugpy",
				"ms-python.python",
				"ms-vscode.vscode-typescript-next",
				"ms-vscode.js-debug-nightly",
				"dbaeumer.vscode-eslint"
			]
		}
	}
}
