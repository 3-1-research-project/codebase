{
	"name": "minitwit-implementations",
	"dockerComposeFile": [
		"./compose.yaml"
	],
	"service": "minitwit-implementations",
	"workspaceFolder": "/workspaces/",
	"postCreateCommand": "bash .devcontainer/minitwit-implementations/post-create.sh",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.black-formatter",
				"ms-python.debugpy",
				"ms-python.python",
				"ms-dotnettools.vscode-dotnet-runtime",
				"ms-dotnettools.csdevkit",
				"ms-dotnettools.csharp",
				"ms-dotnettools.vscodeintellicode-csharp",
				"SonarSource.sonarlint-vscode",
				"Shopify.ruby-lsp"
			]
		}
	}
}
